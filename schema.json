{
    "schemaName": "fatecc",
    "schemaTables": {
        "account": {
            "user_id": "VARCHAR(32)",
            "user_name": "VARCHAR(20)",
            "password": "VARCHAR(32)",
            "credit_value": "DECIMAL(2,1)",
            "public_key": "VARCHAR(100)",
            "created_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "user_id",
                "user_name",
                "password",
                "credit_value",
                "public_key",
                "created_time"
            ],
            "PRIMARY KEY": [
                "user_id"
            ]
        },
        "user_basic": {
            "user_id": "VARCHAR(32)",
            "name": "VARCHAR(10)",
            "age": "TINYINT",
            "sex": "VARCHAR(5)",
            "location": "VARCHAR(50)",
            "photo_hash": "VARCHAR(32)",
            "photo_format": "VARCHAR(10)",
            "phone": "VARCHAR(20)",
            "email": "VARCHAR(20)",
            "encrypted_key": "VARCHAR(100)",
            "signature": "VARCHAR(100)",
            "modified_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "user_id",
                "name",
                "age",
                "sex",
                "location",
                "photo_hash",
                "photo_format",
                "phone",
                "email",
                "encrypted_key",
                "signature",
                "modified_time"
            ],
            "PRIMARY KEY": [
                "user_id"
            ]
        },
        "user_gallery": {
            "hash": "VARCHAR(32)",
            "user_id": "VARCHAR(32)",
            "format": "VARCHAR(10)",
            "encrypted_key": "VARCHAR(100)",
            "signature": "VARCHAR(100)",
            "modified_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "hash",
                "user_id",
                "format",
                "encrypted_key",
                "signature",
                "modified_time"
            ],
            "PRIMARY KEY": [
                "hash"
            ]
        },
        "user_education": {
            "user_id": "VARCHAR(32)",
            "degree": "VARCHAR(20)",
            "school": "VARCHAR(100)",
            "encrypted_key": "VARCHAR(100)",
            "signature": "VARCHAR(100)",
            "modified_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "user_id",
                "degree",
                "school",
                "encrypted_key",
                "signature",
                "modified_time"
            ],
            "PRIMARY KEY": [
                "user_id"
            ]
        },
        "user_occupation": {
            "user_id": "VARCHAR(32)",
            "company": "VARCHAR(50)",
            "job": "VARCHAR(20)",
            "salary": "VARCHAR(100)",
            "encrypted_key": "VARCHAR(100)",
            "signature": "VARCHAR(100)",
            "modified_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "user_id",
                "company",
                "job",
                "salary",
                "encrypted_key",
                "signature",
                "modified_time"
            ],
            "PRIMARY KEY": [
                "user_id"
            ]
        },
        "user_credit": {
            "user_id": "VARCHAR(32)",
            "general": "DECIMAL(2,1)",
            "photo": "DECIMAL(2,1)",
            "education": "DECIMAL(2,1)",
            "occupation": "DECIMAL(2,1)",
            "impression": "DECIMAL(2,1)",
            "other": "DECIMAL(2,1)",
            "date_num": "INTEGER",
            "COLUMN ORDER": [
                "user_id",
                "general",
                "photo",
                "education",
                "occupation",
                "impression",
                "other",
                "date_num"
            ],
            "PRIMARY KEY": [
                "user_id"
            ]
        },
        "date_measure": {
            "sender_id": "VARCHAR(32)",
            "receiver_id": "VARCHAR(32)",
            "general": "TINYINT",
            "photo": "TINYINT",
            "education": "TINYINT",
            "occupation": "TINYINT",
            "impression": "TINYINT",
            "other": "TINYINT",
            "created_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "sender_id",
                "receiver_id",
                "general",
                "photo",
                "education",
                "occupation",
                "impression",
                "other",
                "created_time"
            ],
            "PRIMARY KEY": [
                "sender_id",
                "receiver_id"
            ]
        },
        "date_list": {
            "sender_id": "VARCHAR(32)",
            "receiver_id": "VARCHAR(32)",
            "location": "VARCHAR(20)",
            "date_time": "VARCHAR(14)",
            "message": "VARCHAR(100)",
            "status": "VARCHAR(10)",
            "send_time": "VARCHAR(14)",
            "confirm_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "sender_id",
                "receiver_id",
                "location",
                "date_time",
                "message",
                "status",
                "send_time",
                "confirm_time"
            ],
            "PRIMARY KEY": [
                "sender_id",
                "receiver_id"
            ]
        },
        "permission": {
            "sender_id": "VARCHAR(32)",
            "receiver_id": "VARCHAR(32)",
            "permission_type": "VARCHAR(50)",
            "permission_content": "VARCHAR(50)",
            "status": "VARCHAR(10)",
            "encrypted_key": "VARCHAR(100)",
            "send_time": "VARCHAR(14)",
            "confirm_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "sender_id",
                "receiver_id",
                "permission_type",
                "permission_content",
                "status",
                "encrypted_key",
                "send_time",
                "confirm_time"
            ],
            "PRIMARY KEY": [
                "sender_id",
                "receiver_id",
                "permission_content"
            ]
        },
        "like_list": {
            "user_id": "VARCHAR(32)",
            "liker_id": "VARCHAR(32)",
            "created_time": "VARCHAR(14)",
            "COLUMN ORDER": [
                "user_id",
                "liker_id",
                "created_time"
            ],
            "PRIMARY KEY": [
                "user_id",
                "liker_id"
            ]
        }
    }
}